# Configurare baza de date
spring.datasource.url=<URL BAZA DE DATE>
spring.datasource.username=<UTILIZATOR DB>
spring.datasource.password=<PAROLA DB>
spring.datasource.driver-class-name=org.postgresql.Driver

# Configurare setari de hibernare

spring.jpa.database-platform=org.hibernate.dialect.PostgreSQLDialect
spring.jpa.hibernate.ddl-auto=update

# Configurare dialect PostgreSQL

spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.PostgreSQLDialect

# Configurare log's

logging.level.org.springframework=INFO

# Configurare port de rulare

server.port=8081